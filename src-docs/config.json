{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["src", "src-docs/static/README.md"],
    "includePattern": ".+\\.(js|jsx)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "markdown": {
		"parser": "gfm",
		"hardwrap": true,
		"excludeTags": ["liveexample"]
	},
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "useLongnameInNav": true,
    "applicationName": "nflow",
    "disqus": "",
    "googleAnalytics": "",
    "openGraph": {
      "title": "nflow Documentation",
      "type": "website",
      "image": "logo.svg",
      "site_name": "nflow API Docs",
      "url": ""

    },
    "meta": {
      "title": "",
      "description": "",
      "keyword": ""
    },
    "linenums": true,
    "default":{
      "outputSourceFiles" : true,
      "jsFiles":{
        "include":[
          "dist/nflow.js",
          "nflow-vis.js",
          "babel.js"
        ]
      },
      "cssFiles":{
        "include":[
          "nflow-vis.css",
          "styles.css"
        ]
      },
      "templateFiles":{
        "example":"src-docs/tmpl/example.tmpl",
        "header":"src-docs/tmpl/header.tmpl"
      },
      "staticFiles":{
        "include": [
          "./src-docs/assets"
        ]
      },
      "i18nFile": "src-docs/tmpl/i18n.json",
      "theme": "github"
    }
  },
  "opts": {
    "template": "./node_modules/kitsune-jsdoc-template",
    "encoding": "utf8",
    "destination": "./docs/",
    "private": true,
    "recurse": true,
    "tutorials": "./src-docs/tutorials"
  }
}
